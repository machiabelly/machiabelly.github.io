{
	"name": "polygonjs-editor",
	"version": "1.5.49-1",
	"engines": {
		"node": ">=15.9.0",
		"yarn": ">=1.22.17"
	},
	"main": "src/index.js",
	"license": "Commercial",
	"homepage": "https://polygonjs.com",
	"scripts": {
		"dev": "NODE_ENV=development webpack --env API_ROOT='https://api-npm-editor.polygonjs.com' --config config/webpack/dev.js",
		"run:dev:server": "NODE_ENV=development nodemon build/index.js",
		"build": "NODE_ENV=production webpack --env API_ROOT='https://api-npm-editor.polygonjs.com' --env MARKET_API_ROOT='https://market.polygonjs.com' --config config/webpack/prod.js && yarn fix_index_html && yarn prepare_build_index",
		"fix_index_html": "yarn esbuild_fix_index_html && node config/esbuild/fix_index_html.js",
		"build_dev": "NODE_ENV=production webpack --env API_ROOT='http://localhost:5000' --env MARKET_API_ROOT='http://localhost:3000' --config config/webpack/prod.js && yarn prepare_build_index",
		"prepare_build_index": "echo '#!/usr/bin/env node\n'|cat - build/index.js > /tmp/out && mv /tmp/out build/index.js && chmod +x build/index.js",
		"esbuild": "esbuild src/index.ts --bundle --platform=node --outfile=build/index.js",
		"esbuild_fix_index_html": "esbuild config/esbuild/fix_index_html.ts --bundle --platform=node --outfile=config/esbuild/fix_index_html.js",
		"build_esbuild": "tsc config/esbuild/index.ts",
		"notify_build_complete": "xmessage -center 'BUILD SERVER COMPLETED'",
		"build_pkg_debug": "pkg build/index.js --debug -c ./pkg.json && yarn pkg_rename",
		"build_pkg": "pkg build/index.js --public-packages 'esbuild-linux-64' -c ./pkg.json && yarn pkg_rename",
		"pkg_rename": "mv index build/polygonjs-editor",
		"pkg_debug": "DEBUG_PKG=1 ./build/polygonjs-editor"
	},
	"bin": {
		"polygonjs-editor": "build/index.js"
	},
	"dependencies": {
		"@polygonjs/polygonjs": "1.5.49",
		"btoa": "1.2.1",
		"chalk": "4.1.0",
		"compression-webpack-plugin": "10.0.0",
		"cors": "2.8.5",
		"esbuild": "0.15.7",
		"express": "4.18.1",
		"fflate": "0.7.2",
		"form-data": "4.0.0",
		"fs-extra": "11.1.1",
		"http-status-codes": "2.1.4",
		"lodash": "4.17.21",
		"md5": "2.3.0",
		"ngrok": "4.3.3",
		"node-fetch": "2.6.7",
		"node-machine-id": "1.1.12",
		"prettier": "2.5.1",
		"raw-loader": "4.0.2",
		"socket.io": "4.6.1",
		"tmp": "0.2.1",
		"ts-loader": "9.4.2",
		"ts-shader-loader": "1.0.6",
		"tslib": "2.4.1",
		"typescript": "4.9.4",
		"webpack": "5.76.0",
		"yargs": "16.1.1"
	},
	"devDependencies": {
		"@types/btoa": "1.2.3",
		"@types/cors": "2.8.14",
		"@types/express": "4.17.13",
		"@types/fs-extra": "11.0.1",
		"@types/lodash": "4.14.178",
		"@types/node": "15.9.0",
		"@types/node-fetch": "2.5.7",
		"@types/prettier": "2.4.2",
		"@types/rimraf": "3.0.0",
		"@types/tmp": "0.2.3",
		"@types/yargs": "15.0.12",
		"nodemon": "2.0.22",
		"pkg": "5.8.0",
		"supertest": "6.0.1",
		"terser-webpack-plugin": "5.3.1",
		"uglify-js": "^3.17.2",
		"webpack-cli": "5.0.2",
		"webpack-merge": "5.8.0",
		"webpack-node-externals": "3.0.0",
		"webpack-shell-plugin": "0.5.0"
	}
}
